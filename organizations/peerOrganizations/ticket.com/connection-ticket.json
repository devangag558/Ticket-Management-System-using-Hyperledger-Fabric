{
    "name": "TicketOrgNetwork",
    "version": "1.0.0",
    "client": {
      "organization": "TicketOrg",
      "connection": {
        "timeout": {
          "peer": {
            "endorser": "300"
          },
          "orderer": "300"
        }
      }
    },
    "organizations": {
      "TicketOrg": {
        "mspid": "TicketOrgMSP",
        "peers": [
          "peer0.ticket.com",
          "peer1.ticket.com",
          "peer2.ticket.com",
          "peer3.ticket.com"
        ],
        "certificateAuthorities": [
          "ca.ticket.com"
        ]
      }
    },
    "peers": {
      "peer0.ticket.com": {
        "url": "grpcs://localhost:7051",
        "tlsCACerts": {
          "path": "../../../organizations/peerOrganizations/ticket.com/peers/peer0.ticket.com/tls/server.crt"
        }
      },
      "peer1.ticket.com": {
        "url": "grpcs://localhost:8051",
        "tlsCACerts": {
          "path": "../../../organizations/peerOrganizations/ticket.com/peers/peer1.ticket.com/tls/server.crt"
        }
      },
      "peer2.ticket.com": {
        "url": "grpcs://localhost:9051",
        "tlsCACerts": {
          "path": "../../../organizations/peerOrganizations/ticket.com/peers/peer2.ticket.com/tls/server.crt"
        }
      },
      "peer3.ticket.com": {
        "url": "grpcs://localhost:10051",
        "tlsCACerts": {
          "path": "../../../organizations/peerOrganizations/ticket.com/peers/peer3.ticket.com/tls/server.crt"
        }
      }
    },
    "certificateAuthorities": {
      "ca.ticket.com": {
        "url": "https://localhost:13054",
        "caName": "ca-ticket",
        "tlsCACerts": {
          "path": "../../../organizations/peerOrganizations/ticket.com/ca/ca.ticket.com-cert.pem"
        },
        "httpOptions": {
          "verify": false
        }
      }
    },
    "orderers": {
      "orderer1.orderer.com": {
        "url": "grpcs://localhost:7050",
        "tlsCACerts": {
          "path": "../../../crypto-config/ordererOrganizations/orderer.com/orderers/orderer1.orderer.com/tls/ca.crt"
        },
        "grpcOptions": {
          "ssl-target-name-override": "orderer1.orderer.com",
          "hostnameOverride": "orderer1.orderer.com"
        }
      }
    }
  }
  